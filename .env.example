# Environment Configuration
# Скопируйте в .env и заполните значения

# Режим работы
DEBUG=false
ENVIRONMENT=production

# База данных (SQLite)
DATABASE_PATH=./data/ai_service.db

# AI API ключи (опционально)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# Telegram (опционально)
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CLIENT_BOT_TOKEN=your_client_bot_token_here
TELEGRAM_MASTER_BOT_TOKEN=your_master_bot_token_here

# Платежи (опционально)
YOOKASSA_SHOP_ID=your_shop_id
YOOKASSA_SECRET_KEY=your_secret_key

# Комиссия платформы (25% по умолчанию)
PLATFORM_COMMISSION_RATE=0.25

# Порт (для локального запуска)
PORT=8000
